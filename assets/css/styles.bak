    .top h1 {
      color: #4b575a;
      line-height: 40px;
      text-shadow: 0 1px 1px rgba(0,0,0,.4);
    }
    
    .top p {
      color: #4b575a;
      line-height: 30px;
      margin-bottom: 15px;
      margin-top: 20px;      
    }
    
    .top {
      width: 45%;
      margin-left: 0;
    }
    
    .jumbotron {
        background-color: white;
        background-position: right top;
        background-repeat: no-repeat;
        padding: 100px;
    }


    /* MARKETING CONTENT
    -------------------------------------------------- */

    /* Center align the text within the three columns below the carousel */
    .marketing .span4 {
      text-align: center;
    }
    .marketing h2 {
      font-weight: normal;
    }
    .marketing .span4 p {
      margin-left: 10px;
      margin-right: 10px;
    }


    /* Featurettes
    ------------------------- */

    .featurette-divider {
      margin: 40px 0; /* Space out the Bootstrap <hr> more */
    }
    .featurette {
      /*padding-top: 40px;  Vertically center images part 1: add padding above and below text. */
      overflow: hidden; /* Vertically center images part 2: clear their floats. */
    }
    .featurette-image {
      margin-top: -120px; /* Vertically center images part 3: negative margin up the image the same amount of the padding to center it. */
    }

    /* Give some space on the sides of the floated elements so text doesn't run right into it. */
    .featurette-image.pull-left {
      margin-right: 40px;
    }
    .featurette-image.pull-right {
      margin-left: 40px;
    }

    /* Thin out the marketing headings */
    .featurette-heading {
      font-size: 70px;
      font-weight: 300;
      line-height: 1;
      letter-spacing: -1px;
      text-align: center;
    }
    img.tech-icon {
      width: 5em;
      padding: 20px;
    }
    img.headshot {
      height:16em;
      padding: 0.2em;
    }
    .lead {
      padding-right: 220px;
      padding-left: 220px;
      text-align: center;
    }
    
    .toplead {
      font-size: 21px;
      font-weight: 200;
    }

    #attend {
      background-image: url("../assets/img/typing-image.png");
      color: whitesmoke;
      padding: 10em 20em;
      margin: -40px -130px;
      opacity: 0.4;
      filter: alpha(opacity=40); /*For IE8 and earlier*/
      background-repeat: no-repeat;
      background-size: 100%;
    }
    
    #start {
      
      background-image: url("../assets/img/student1.jpeg");
      color: black;
      padding: 10em 55% 10em 20em;
      opacity: 0.4;
      filter: alpha(opacity=40); /*For IE8 and earlier*/
      margin: -40px -130px;
      background-repeat: no-repeat;
      background-size: 100%;
      
    }



    /* RESPONSIVE CSS
    -------------------------------------------------- */

    @media (max-width: 979px) {

      .container.navbar-wrapper {
        margin-bottom: 0;
        width: auto;
      }
      .navbar-inner {
        border-radius: 0;
        margin: -20px 0;
      }
/*
      .top .item {
        height: 500px;
      }
      .top img {
        width: auto;
        height: 500px;
      }
*/
      .featurette {
        height: auto;
        padding: 0;
        margin-left: -20px;
        margin-right: -20px;
      }
      .featurette-image.pull-left,
      .featurette-image.pull-right {
        display: block;
        float: none;
        max-width: 40%;
        margin: 0 auto 20px;
      }
      .top p {
        font-size: 20px;
        line-height: 26px;
      }
      .top h1 {
        font-size: 26px;
        line-height: 32px;
      }
    }

    @media (max-width: 767px) {

      .navbar-inner {
       /*(margin: -20px;*/
      }
      .top {
        width: 100%;
      }
      .top p {
        font-size: 20px;
        line-height: 24px;
        color: black;
        font-weight: 400;
      }
      .top h1 {
        font-size: 24px;
        line-height: 28px;
        color:black; 
      }
      .jumbotron {
        background-position: center top;
        padding: 100px 50px;
      }
